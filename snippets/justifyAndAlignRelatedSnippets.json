{
  "All Justify Properties":{
    "description": "All Justify Properties",
    "prefix": "!all-justify",
    "body":[
      "justify-content: ${1:center};",
      "justify-items: ${2:center};",
      "justify-self: ${3:center};"
    ]
  },
  "All Align Properties":{
    "description": "All Align Properties",
    "prefix": "!all-align",
    "body":[
      "align-content: ${1:center};",
      "align-items: ${2:center};",
      "align-self: ${3:center};"
    ]
  },
  "All Justify And Align Properties":{
    "description": "All Justify And Align Properties",
    "prefix": "!all-justify-align",
    "body":[
      "justify-content: ${1:center};",
      "justify-items: ${2:center};",
      "justify-self: ${3:center};",
      "align-content: ${4:center};",
      "align-items: ${5:center};",
      "align-self: ${6:center};"
    ]
  },
  "Justify Content And Align Items Center":{
    "description": "Set Justify Content And Align Items To Center",
    "prefix": "!justify-align-center",
    "body":[
      "justify-content: ${1:center};",
      "align-items: ${2:center};"
    ]
  }
}