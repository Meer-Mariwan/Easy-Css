{
  "Opacity":{
    "description": "Set Opacity Property To Zero",
    "prefix": "!opacity",
    "body":[
      "opacity: ${1:0};"
    ]
  },
  "Visibility":{
    "description": "Set Visibility Property To Hidden",
    "prefix": "!visibility",
    "body":[
      "visibility: ${1:hidden};"
    ]
  },
  "Opacity And Visibility":{
    "description": "Opacity And Visibility Properties",
    "prefix": "!opacity-visibility",
    "body":[
      "opacity: ${1:0};",
      "visibility: ${2:hidden};"
    ]
  }
}